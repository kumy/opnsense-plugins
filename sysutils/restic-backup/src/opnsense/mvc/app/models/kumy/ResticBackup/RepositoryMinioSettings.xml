<model>
    <mount>//system/backup/restic/repositories/minio</mount>
    <description>OPNsense Restic Backup Settings</description>
    <version>0.0.0</version>
    <items>
        <enabled type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
            <Constraints>
                <check001>
                    <reference>url.check001</reference>
                </check001>
                <check002>
                    <reference>aws_access_key_id.check001</reference>
                </check002>
                <check003>
                    <reference>aws_secret_access_key.check001</reference>
                </check003>
            </Constraints>
        </enabled>
        <url type="TextField">
            <Required>N</Required>
            <Constraints>
                <check001>
                    <ValidationMessage>An URL is required.</ValidationMessage>
                    <type>DependConstraint</type>
                    <addFields>
                        <field1>enabled</field1>
                    </addFields>
                </check001>
            </Constraints>
        </url>
        <aws_access_key_id type="TextField">
            <Required>N</Required>
            <Constraints>
                <check001>
                    <ValidationMessage>An AWS_ACCESS_KEY_ID is required.</ValidationMessage>
                    <type>DependConstraint</type>
                    <addFields>
                        <field1>enabled</field1>
                    </addFields>
                </check001>
            </Constraints>
        </aws_access_key_id>
        <aws_secret_access_key type="TextField">
            <Required>N</Required>
            <Constraints>
                <check001>
                    <ValidationMessage>An AWS_SECRET_ACCESS_KEY is required.</ValidationMessage>
                    <type>DependConstraint</type>
                    <addFields>
                        <field1>enabled</field1>
                    </addFields>
                </check001>
            </Constraints>
        </aws_secret_access_key>
        <repositorypassword type=".\TextOrGlobalDefaultField">
            <Required>Y</Required>
        </repositorypassword>
        <advanced>
            <no_cache type="OptionField">
                <default>global default</default>
                <required>Y</required>
                <OptionValues>
                    <enabled value="1">on</enabled>
                    <disabled value="0">off</disabled>
                    <global_default value="global default">global default</global_default>
                </OptionValues>
            </no_cache>
            <cache_dir type=".\TextOrGlobalDefaultField" />
            <insecure_tls type="OptionField">
                <default>global default</default>
                <required>Y</required>
                <OptionValues>
                    <enabled value="1">on</enabled>
                    <disabled value="0">off</disabled>
                    <global_default value="global default">global default</global_default>
                </OptionValues>
            </insecure_tls>
            <cleanup_cache type="OptionField">
                <default>global default</default>
                <required>Y</required>
                <OptionValues>
                    <enabled value="1">on</enabled>
                    <disabled value="0">off</disabled>
                    <global_default value="global default">global default</global_default>
                </OptionValues>
            </cleanup_cache>
            <compression type="OptionField">
                <default>global default</default>
                <required>Y</required>
                <OptionValues>
                    <off>off</off>
                    <max>max</max>
                    <global_default value="global default">global default</global_default>
                </OptionValues>
            </compression>
            <http_user_agent type=".\TextOrGlobalDefaultField" />
            <retry_lock type=".\TextOrGlobalDefaultField" />
            <no_extra_verify type="OptionField">
                <default>global default</default>
                <required>Y</required>
                <OptionValues>
                    <enabled value="1">on</enabled>
                    <disabled value="0">off</disabled>
                    <global_default value="global default">global default</global_default>
                </OptionValues>
            </no_extra_verify>
            <limit_download type=".\IntegerFieldOrGlobalDefaultField">
                <default>global default</default>
            </limit_download>
            <limit_upload type=".\IntegerFieldOrGlobalDefaultField">
                <default>global default</default>
            </limit_upload>
            <stuck_request_timeout type=".\TextOrGlobalDefaultField" />
        </advanced>
    </items>
</model>
